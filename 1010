// TC O(N), SC O(N)

class Solution {
public:
    int numPairsDivisibleBy60(vector<int>& time) {
        vector<int> remainderHash(60, 0);
        int result = 0;
        
        for(int& t: time) {
            result += remainderHash[(60 - (t % 60)) % 60];
            remainderHash[t % 60]++;
        }
        
        return result;
    }
};
