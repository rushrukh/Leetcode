class Solution {
public:
    int DP[101][101];
    int uniquePaths(int m, int n) {
        memset(DP, -1, sizeof(DP));
        return helper(m, n, 0, 0);
    }
    int helper(int m, int n, int i, int j) {
        if(i >= m || j >= n) return 0;
        if(i == m - 1 && j == n - 1) return 1;
        if(DP[i][j] != -1) return DP[i][j];
        
        return DP[i][j] = helper(m, n, i, j + 1) + helper(m, n, i + 1, j); 
    }
};
