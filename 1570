// TC O(N), SC O(1)

class SparseVector {
public:
    vector<int> v1;
    SparseVector(vector<int> &nums) {
        v1 = nums;
    }
    
    // Return the dotProduct of two sparse vectors
    int dotProduct(SparseVector& vec) {
        int result = 0;
        
        for(int i = 0; i < v1.size(); i++)
            result += v1[i] * vec.v1[i];
        
        return result;
    }
};
