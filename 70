// Top-down: TC O(N), SC O(N)

class Solution {
public:
    vector<int> DP;
    int climbStairs(int n) {
        DP.assign(n + 1, -1);
        return dp(n);
    }
    
    int dp(int n) {
        if(n == 1) return 1;
        if(n == 2) return 2;
        if(DP[n] > -1) return DP[n];
        
        return DP[n] = dp(n - 1) + dp(n - 2);
    }
};
